<style>
	.nc_table {
		font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
		width: 100%;
		border-collapse: collapse;
	}
	.nc_table td, .nc_table th {
		font-size: 1em;
		border: 1px solid #2E2E2E;
		padding: 3px 7px 2px 7px;
		width: auto
	}
	.nc_table th {
		font-size: 1.1em;
		text-align: left;
		padding-top: 5px;
		padding-bottom: 4px;
		background-color: #777777;
		color: #000000;
	}

</style>
<% if @tests.size > 0 %>
<div style="margin-bottom: 0px; margin-top: -45px">
	<div style="float: left">
		<h4>Test Reports:</h4>
	</div>	
	<div style="clear: both"></div>
</div>

<table cellpadding="0" cellspacing="0" border="0" class="display" id="dataTable">
	<thead>
		<tr>
			<th width="300px">Test</th>
			<th width="300px">Start Date</th>
			<th width="300px">End Date</th>
			<th width="300px">Max Score</th>
			<th width="200px"></th>
		</tr>
	</thead>
	<tbody>
		<% @tests.each do |test| %>
		<tr>
			<td width="300px"><%= test.name %></td>
			<td width="300px"><%= test.start_date %></td>
			<td width="300px"><%= test.end_date %></td>
			<td width="300px"><%= test.max_score %></td>
			<!--<td width="200px"><%= test.level_id %></td-->
			<td><a href="/students/view_report?test_id=<%= test.id%>" target="_blank">View</a> &nbsp; | &nbsp; <%= link_to "Download", {:controller => "admins", :action => "view_report", :test_id => test.id, :format => "pdf"} %></td>
			<!--td width="200px"><a href="/students/view_report?test_id=<%= test.id%>" target="_blank">View Report</td-->
			<!-- <td width="200px"><a href="/students/view_report?test_id=<%=test.id%>" target="_blank">View Report</td> -->
		</tr>
		<% end %>
	</tbody>
</table>
<% else %>
<p>
	There are no tests for your level.
</p>
<% end %>
