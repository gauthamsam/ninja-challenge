<table>
  <tr>
    <th>Test Name</th>
    <th>Start Date</th>
    <th>End Date</th>
    <th>Max Score</th>
    <th>Level</th>    
  </tr>

<% @tests.each do |test| %>
  <tr>
    <td><%= test.name %></td>
    <td><%= test.start_date %></td>
    <td><%= test.end_date %></td>
    <td><%= test.max_score %></td>
    <td><%= test.level_id %></td>
    <td><%= link_to 'Edit', '' %></td>
    <td><%= link_to 'Destroy', test, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
